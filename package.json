{
  "name": "medinsky",
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently --kill-others \"npm:start:frontend\" \"npm:start:backend\"",
    "start:frontend": "ng serve --ssl true",
    "start:backend": "node dist/server.js",
    "build": "ng build --prod --build-optimizer && yarn build:backend",
    "build:backend": "tsc -p \"backend/tsconfig.json\" && copyfiles -u 1 backend/server.crt dist/ && copyfiles -u 1 backend/server.csr dist/ && copyfiles -u 1 backend/server.key dist/ && copyfiles -u 1 backend/users.json dist/",
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "body-parser": "1.18.3",
    "bootstrap": "4.1.3",
    "compression": "1.7.3",
    "core-js": "^2.5.4",
    "express": "4.16.3",
    "rxjs": "~6.2.0",
    "spdy": "3.4.7",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/shelljs": "0.8.0",
    "@types/spdy": "3.4.4",
    "@types/webpack": "4.4.13",
    "codelyzer": "~4.3.0",
    "concurrently": "4.0.1",
    "copyfiles": "2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "7.0.1",
    "tslint": "~5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "~2.9.2"
  }
}
